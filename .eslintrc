{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	},
	"extends": "airbnb",
	"settings": {
		"ecmascript": 6,
		"jsx": true
	},
	"rules": {
    "indent": [2, "tab", {"SwitchCase": 1}],
    "react/jsx-indent-props": [2, "tab"],
    "no-confusing-arrow": 0,
    "eqeqeq": ["error", "allow-null"]
	}
}
